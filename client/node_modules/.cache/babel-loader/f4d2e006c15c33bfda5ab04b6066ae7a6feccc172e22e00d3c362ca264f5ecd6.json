};\n  },\n  async create() {\n    try {\n      this.posts = await PostService.getPosts();\n      console.log(1);\n    } catch (err) {\n      this.error = err.message;\n    }\n  },\n  methods: {\n    async createPost() {\n      await PostService.insertPost(this.text);\n      this.posts = await PostService.getPosts();\n    },\n    async deletePost(id) {\n      await PostService.deletePost(id);\n      this.posts = await PostService.getPosts();\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\ndiv.container {\n  max-width: 800px;\n  margin: 0 auto;\n}\n\np.error {\n  border: 1px solid #ff5b5f;\n  background-color: #ffc5c1;\n  padding:  10px;\n  margin-bottom: 15px;\n}\n\ndiv.post {\n  position: relative;\n  border: 1px solid #5bd658;\n  background-color:  #bcffb8;\n  padding: 10px 10px 30px 10px;\n  margin-bottom: 15px;\n}\n\ndiv.created-at {\n  position: absolute;\n  top: 0;\n  left: 0;\n  padding: 5px 15px 5px 15px;\n  background-color: darkgreen;\n  color: white;\n  font-size: 13px;\n}\n\np.text {\n  font-size: 22px;\n  font-weight: 700;\n  margin-bottom: 0;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,WAAU,MAAO,gBAAgB;AAExC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACD,MAAMC,MAAMA,CAAA,EAAG;IACb,IAAI;MACF,IAAI,CAACH,KAAI,GAAI,MAAMH,WAAW,CAACO,QAAQ,EAAE;MACzCC,OAAO,CAACC,GAAG,CAAC,CAAC,CAAC;IAChB,EAAE,OAAOC,GAAG,EAAE;MACZ,IAAI,CAACN,KAAI,GAAIM,GAAG,CAACC,OAAO;IAC1B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,MAAMb,WAAW,CAACc,UAAU,CAAC,IAAI,CAACT,IAAI,CAAC;MACvC,IAAI,CAACF,KAAI,GAAI,MAAMH,WAAW,CAACO,QAAQ,EAAE;IAC3C,CAAC;IACD,MAAMQ,UAAUA,CAACC,EAAE,EAAE;MACnB,MAAMhB,WAAW,CAACe,UAAU,CAACC,EAAE,CAAC;MAChC,IAAI,CAACb,KAAI,GAAI,MAAMH,WAAW,CAACO,QAAQ,EAAE;IAC3C;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}