\n      this.posts = await PostService.getPosts();\n    } catch (err) {\n      this.error = err.message;\n    }\n  },\n  methods: {\n    async createPost() {\n      await PostService.insertPost(this.text);\n      this.posts = await PostService.getPosts();\n    },\n    async deletePost(id) {\n      await PostService.deletePost(id);\n      this.posts = await PostService.getPosts();\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,WAAU,MAAO,gBAAgB;AAExC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACD,MAAMC,MAAMA,CAAA,EAAG;IACb,IAAI;MACF,IAAI,CAACH,KAAI,GAAI,MAAMH,WAAW,CAACO,QAAQ,EAAE;IAC3C,EAAE,OAAOC,GAAG,EAAE;MACZ,IAAI,CAACJ,KAAI,GAAII,GAAG,CAACC,OAAO;IAC1B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,MAAMX,WAAW,CAACY,UAAU,CAAC,IAAI,CAACP,IAAI,CAAC;MACvC,IAAI,CAACF,KAAI,GAAI,MAAMH,WAAW,CAACO,QAAQ,EAAE;IAC3C,CAAC;IACD,MAAMM,UAAUA,CAACC,EAAE,EAAE;MACnB,MAAMd,WAAW,CAACa,UAAU,CAACC,EAAE,CAAC;MAChC,IAAI,CAACX,KAAI,GAAI,MAAMH,WAAW,CAACO,QAAQ,EAAE;IAC3C;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}